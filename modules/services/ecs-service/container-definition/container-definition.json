[
  {
    "name": "${container_name}",
    "image": "${image}:${version}",
    "cpu": ${cpu},
    "memory": ${memory},
    "essential": true,
    "portMappings": ${port_mappings},
    "environment": ${env_vars},
    "logConfiguration": {
      "logDriver": "syslog",
      "options": {
        "tag": "${container_name} ({{"{{"}}.ID{{"}}"}})"
      }
    }
    {{- if .UseCustomDockerRunCommand }},
    "command": [${command}]
    {{- end }}
  }
]
